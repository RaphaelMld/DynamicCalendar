{
  "name": "dynamiccalendar",
  "version": "1.0.0",
  "description": "Static timetable site built from CalDAV feeds and deployed via GitHub Pages",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist && mkdir -p public",
    "build": "node scripts/build.js",
    "start": "npx http-server public -p 5173 -c-1",
    "prepare": "npm run build"
  },
  "keywords": [
    "calendar",
    "caldav",
    "ics",
    "github-pages"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ical.js": "^2.1.0",
    "node-fetch": "^3.3.2",
    "tsdav": "^2.0.5"
  },
  "devDependencies": {
    "http-server": "^14.1.1"
  }
}
